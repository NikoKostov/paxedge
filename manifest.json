{
  "manifest_version": 2,
  "name": "Number Switcher",
  "version": "1.0",
  "description": "Extension that performs number manipulation.",
  "icons": {
    "48": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
